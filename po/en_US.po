#: src/hangeulconv.c:82
#, c-format
msgid "Try `%s --help`' for more information.\n"
msgstr "Try `%s --help`' for more information.\n"

#: src/hangeulconv.c:85
#, c-format
msgid "Usage: %s [OPTION]... [FILE]...\n"
msgstr "Usage: %s [OPTION]... [FILE]...\n"

#: src/hangeulconv.c:86
msgid "Convert hangeul to romaja characters or romaja to hangeul.\n"
msgstr "Convert hangeul to romaja characters or romaja to hangeul.\n"

#: src/hangeulconv.c:88
msgid ""
"  -t, --transliteration-system=SYSTEM   use transliteration system SYSTEM:\n"
"                                          yale (y), revised-romanization "
"(r),\n"
"                                          mccune-reischauer (m), kontsevich\n"
"                                          (k), skats (s)\n"
msgstr ""
"  -t, --transliteration-system=SYSTEM   use transliteration system SYSTEM:\n"
"                                          yale (y), revised-romanization "
"(r),\n"
"                                          mccune-reischauer (m), kontsevich\n"
"                                          (k), skats (s)\n"

#: src/hangeulconv.c:94
msgid "  -o, --output=FILE                     print output to FILE\n"
msgstr "  -o, --output=FILE                     print output to FILE\n"

#: src/hangeulconv.c:97
msgid "  -i, --input=FILE                      get input from FILE\n"
msgstr "  -i, --input=FILE                      get input from FILE\n"

#: src/hangeulconv.c:100
msgid ""
"  -v, --version                         print version information and quit\n"
msgstr ""
"  -v, --version                         print version information and quit\n"

#: src/hangeulconv.c:103
msgid ""
"  -h, --help                            print this help message and quit\n"
msgstr ""
"  -h, --help                            print this help message and quit\n"
